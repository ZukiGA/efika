{
  "name": "Efika",
  "displayName": "Efika",
  "expo": {
    "name": "Efika",
    "slug": "Efika",
    "version": "0.5.0",
    "orientation": "portrait",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.efika",
      "permissions": ["READ_PHONE_STATE", "RECEIVE_BOOT_COMPLETED"],
      "versionCode": 2
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./local/path/to/myNotificationIcon.png",
          "color": "#ffffff",
          "sounds": ["./local/path/to/mySound.wav", "./local/path/to/myOtherSound.wav"],
          "mode": "production"
        }
      ]
    ]
  }
}
