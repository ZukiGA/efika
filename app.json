{
  "name": "Efika",
  "displayName": "Efika",
  "expo": {
    "name": "Efika",
    "icon": "./logo.png",
    "slug": "Efika",
    "version": "0.5.5",
    "orientation": "portrait",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.efika",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./logo.png",
        "backgroundColor": "#DADADA"
      },
      "versionCode": 5
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/bell.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/pop.wav"
          ],
          "mode": "production"
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.efika"
    }
  }
}
